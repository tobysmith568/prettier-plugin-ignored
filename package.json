{
  "name": "prettier-plugin-ignored",
  "version": "0.0.0",
  "description": "A Prettier plugin that adds an 'ignored' language, allowing you to ignore files without listing them in a .prettierignore file.",
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "prettier": "@tobysmith568/prettier-config",
  "type": "module",
  "keywords": [
    "prettier",
    "prettier-plugin",
    "configuration",
    "format",
    "autoformat",
    "autoformatter",
    "ignore",
    "prettierignore"
  ],
  "author": {
    "name": "Toby Smith",
    "email": "contact@tobythe.dev",
    "url": "https://tobysmith.uk"
  },
  "homepage": "https://github.com/tobysmith568/prettier-plugin-ignored",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobysmith568/prettier-plugin-ignored.git"
  },
  "bugs": {
    "url": "https://github.com/tobysmith568/prettier-plugin-ignored/issues"
  },
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "dependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "prettier": "3.x"
  },
  "devDependencies": {
    "@license-cop/permissive": "^1.8.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@tobysmith568/eslint-config": "^2.3.0",
    "@tobysmith568/prettier-config": "^2.2.1",
    "license-cop": "^1.8.0",
    "rollup": "^4.34.7",
    "rollup-plugin-delete": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.0"
  }
}
