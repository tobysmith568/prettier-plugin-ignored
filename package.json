{
  "name": "prettier-plugin-ignored",
  "version": "0.0.0",
  "description": "A Prettier plugin that adds an 'ignored' language, allowing you to ignore files without listing them in a .prettierignore file.",
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "prettier": "@tobysmith568/prettier-config",
  "type": "module",
  "keywords": [
    "prettier",
    "prettier-plugin",
    "configuration",
    "format",
    "autoformat",
    "autoformatter",
    "ignore",
    "prettierignore"
  ],
  "author": {
    "name": "Toby Smith",
    "email": "contact@tobythe.dev",
    "url": "https://tobysmith.uk"
  },
  "homepage": "https://github.com/tobysmith568/prettier-plugin-ignored",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobysmith568/prettier-plugin-ignored.git"
  },
  "bugs": {
    "url": "https://github.com/tobysmith568/prettier-plugin-ignored/issues"
  },
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "dependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "prettier": "3.x"
  },
  "devDependencies": {
    "@license-cop/permissive": "^1.8.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@tobysmith568/eslint-config": "^2.3.0",
    "@tobysmith568/prettier-config": "^2.2.1",
    "license-cop": "^1.8.0",
    "rollup": "^4.34.7",
    "rollup-plugin-delete": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
