{
  "name": "prettier-plugin-ignored",
  "version": "0.0.0",
  "description": "A Prettier plugin that adds an 'ignored' language, allowing you to ignore files without listing them in a .prettierignore file.",
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a",
  "prettier": "@tobysmith568/prettier-config",
  "type": "module",
  "keywords": [
    "prettier",
    "prettier-plugin",
    "configuration",
    "format",
    "autoformat",
    "autoformatter",
    "ignore",
    "prettierignore"
  ],
  "author": {
    "name": "Toby Smith",
    "email": "contact@tobythe.dev",
    "url": "https://tobysmith.uk"
  },
  "homepage": "https://github.com/tobysmith568/prettier-plugin-ignored",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobysmith568/prettier-plugin-ignored.git"
  },
  "bugs": {
    "url": "https://github.com/tobysmith568/prettier-plugin-ignored/issues"
  },
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "dependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "prettier": "3.x"
  },
  "devDependencies": {
    "@license-cop/permissive": "^1.6.2",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@tobysmith568/prettier-config": "^2.1.1",
    "license-cop": "^1.6.2",
    "rollup": "^4.21.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
